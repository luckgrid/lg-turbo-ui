{
  "name": "docs",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "env": "dotenvx",
    "env:encrypt": "dotenvx encrypt -k '*_SECRET_API_TOKEN'",
    "env:debug": "dotenvx get --all --pretty-print",
    "env:run": "dotenvx run --",
    "env:run:debug": "pnpm env:debug && dotenvx run --debug -f .env* --",
    "dev": "pnpm env:run next dev --turbopack --port 3001",
    "build": "pnpm env:run next build",
    "start": "pnpm env:run next start --port 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "clean": "git clean -xdf .next .turbo node_modules"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "catalog:",
    "@hookform/resolvers": "catalog:",
    "@icons-pack/react-simple-icons": "catalog:",
    "@workspace/ui": "workspace:*",
    "@workspace/ui-next": "workspace:*",
    "airtable": "catalog:",
    "lucide-react": "catalog:",
    "next": "catalog:",
    "next-themes": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "catalog:"
  }
}
