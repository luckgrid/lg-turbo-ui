{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "dev": {
      "env": ["AIRTABLE_*", "APP_*", "DOTENV_*"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "persistent": true
    },
    "build": {
      "env": ["AIRTABLE_*", "APP_*", "DOTENV_*"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "start": {
      "env": ["AIRTABLE_*", "APP_*", "DOTENV_*"],
      "persistent": true
    }
  }
}
