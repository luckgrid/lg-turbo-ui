{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "env:encrypt": "dotenvx encrypt -k '*_SECRET_API_TOKEN'",
    "env:debug": "dotenvx get --all --pretty-print",
    "env:run": "dotenvx run --",
    "env:run:dev": "pnpm env:debug && dotenvx run --debug -f .env* --",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm env:run:dev next dev --turbopack --port 3000",
    "build": "pnpm env:run next build",
    "start": "pnpm env:run next start --port 3000",
    "format": "prettier --check . --ignore-path .prettierignore --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc"
  },
  "prettier": "@workspace/prettier-config",
  "dependencies": {
    "@dotenvx/dotenvx": "catalog:",
    "@icons-pack/react-simple-icons": "catalog:",
    "@workspace/ui": "workspace:*",
    "@workspace/ui-next": "workspace:*",
    "airtable": "catalog:",
    "lucide-react": "catalog:",
    "next": "catalog:",
    "next-themes": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/prettier-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
